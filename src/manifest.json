{
  "name": "Coding Blocks URL shortener",
  "description": "Shorts the current url in the current tab using cb service ",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "matches": ["<all_urls>"],
      "exclude_matches": [],
      "js": [
        "jquery.js"
      ]
    }
  ]
}